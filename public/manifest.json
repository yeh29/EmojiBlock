{
  "name": "EmojiBlock",
  "description": "Replaces ads with emojis.",
  "manifest_version": 2,
  "version": "1.0",
  "options_page": "index.html",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_title": "EmojiBlock"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'sha256-4HVgL2oJmM6rR9kSS8Bd/mDsWIQl11jqWf/mV20LlEY='; object-src 'self'"
}
